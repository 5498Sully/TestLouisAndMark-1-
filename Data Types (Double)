class Kaiser {
  public static void main(string args[]) {
   int var;
   double x;
// i just changed it
var = 10;

x = 10.0;

System.out.println("original value of var: " + var);
System.out.println("original value of x: " + x);
System.out.println();

var = var / 4;
x = x / 4;

System.out.println("var after division: " + var);
System.out.println("x after division: " + x);
   }
}
